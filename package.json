    "cors": "^2.8.5",
    "express": "^4.17.3",
    "graphql": "^14.3.1",
    "http": "^0.0.0",
    "ncp": "^2.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/graphql": "^14.2.0",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/jest": "^24.0.14",
    "@types/node": "^12.0.8",
    "@types/supertest": "^2.0.7",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "^0.0.4",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2"
  }
